{
    "name": "redux-intensive-boilerplate",
    "version": "2.0.0",
    "author": "Dima Vakatsiienko <dvakatsiienko@lectrum.io>",
    "private": true,
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/webpack/serve.js\"",
        "build": "cross-env-shell NODE_ENV=production node --require @babel/register \"./scripts/webpack/build.js\"",
        "sync": "cross-env-shell NODE_ENV=node node --require @babel/register \"./scripts/git/sync.js\"",
        "build:github-pages": "cross-env-shell NODE_ENV=production DEPLOY_TARGET=github-pages npm run build",
        "build:analyze": "run-s build analyze",
        "analyze": "webpack-bundle-analyzer \"./build/build-stats.json\" build",
        "lint": "run-s lint:javascript lint:css",
        "lint:javascript": "jest --projects \"./jest/jest.eslint.config.js\"",
        "lint:css": "jest --projects \"./jest/jest.stylelint.config.js\"",
        "test": "jest --config=\"./jest/jest.test.config.js\"",
        "test:watch": "npm test -- --watch",
        "test:debug": "node --inspect-brk jest --runInBand --config=\"./jest/jest.test.config.js\"",
        "soundcheck": "jest --config=\"./jest/jest.config.js\"",
        "prettier": "prettier-eslint --list-different \"./source/**/*.js\"; prettier-eslint --write \"./source/**/*.js\"",
        "deploy": "run-s build:github-pages gh-pages",
        "gh-pages": "gh-pages -d build",
        "commit": "npx git-cz"
    },
    "dependencies": {
        "classnames": "2.2.6",
        "connected-react-router": "^4.4.1",
        "formik": "1.3.0",
        "gsap": "2.0.2",
        "history": "4.7.2",
        "immutable": "4.0.0-rc.9",
        "moment": "2.22.2",
        "react": "16.5.0",
        "react-dom": "16.5.0",
        "react-flip-move": "3.0.2",
        "react-redux": "5.0.7",
        "react-redux-form": "1.16.10",
        "react-router-dom": "4.3.1",
        "react-router-redux": "5.0.0-alpha.8",
        "react-transition-group": "2.4.0",
        "redux": "4.0.0",
        "redux-saga": "0.16.0",
        "redux-thunk": "2.3.0",
        "reset-css": "4.0.1",
        "socket.io-client": "2.1.1",
        "uuid": "3.3.2",
        "yup": "0.26.5"
    },
