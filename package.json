        "babel-eslint": "9.0.0",
        "babel-jest": "23.6.0",
        "babel-loader": "8.0.2",
        "babel-plugin-dynamic-import-node": "2.1.0",
        "chalk": "2.4.1",
        "clean-webpack-plugin": "0.1.19",
        "commitizen": "2.10.1",
        "connect-history-api-fallback": "1.5.0",
        "cross-env": "5.2.0",
        "css-loader": "1.0.0",
        "css-mqpacker": "7.0.0",
        "cssnano": "4.1.0",
